          "404": {
            "description": "Concesionario no encontrado"
          },
          "500": {
            "description": "Error en el servidor"
          }
        }
      }
    },
    "/concesionarios/{id}/coches": {
      "get": {
        "summary": "Obtener todos los coches de un concesionario por su ID",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "ID del concesionario",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
