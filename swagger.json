              "application/json": {
                "example": {}
              }
            }
          },
          "404": {
            "description": "Concesionario no encontrado"
          },
          "500": {
            "description": "Error en el servidor"
          }
        }
      },
      "put": {
        "summary": "Actualizar un concesionario por su ID",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "ID del concesionario",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Concesionario actualizado correctamente"
          },
          "404": {
            "description": "Concesionario no encontrado"
          },
          "500": {
            "description": "Error en el servidor"
          }
        }
      },
      "delete": {
        "summary": "Borrar un concesionario por su ID",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "ID del concesionario",
