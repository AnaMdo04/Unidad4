{
  "openapi": "3.0.0",
  "info": {
    "title": "Concesionario API",
    "version": "1.0.0",
    "description": "API para gestionar concesionarios y coches"
  },
  "servers": [
    {
      "url": "http://localhost:8080",
      "description": "Servidor local"
    }
  ],
  "paths": {
    "/concesionarios": {
      "get": {
        "summary": "Obtener todos los concesionarios",
        "responses": {
          "200": {
            "description": "Lista de concesionarios",
            "content": {
              "application/json": {
                "example": []
              }
            }
          }
        }
      },
      "post": {
        "summary": "Crear un nuevo concesionario",
        "responses": {
          "201": {
            "description": "Concesionario creado correctamente"
          },
          "500": {
            "description": "Error en el servidor"
          }
        }
      }
    },
    "/concesionarios/{id}": {
      "get": {
        "summary": "Obtener un concesionario por su ID",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "ID del concesionario",
            "schema": {
              "type": "string"
            }
          }
